            <MainSource>
              <DbSource ConnectionRef="CINE_CORDOBAConnectionString (Settings)" DbObjectType="Unknown" FillMethodModifier="Public" FillMethodName="Fill" GenerateMethods="Both" GenerateShortCommands="false" GeneratorGetMethodName="GetData" GeneratorSourceName="Fill" GetMethodModifier="Public" GetMethodName="GetData" QueryType="Rowset" ScalarCallRetval="System.Object, mscorlib, Version=4.0.0.0, Culture=neutral, PublicKeyToken=b77a5c561934e089" UseOptimisticConcurrency="false" UserGetMethodName="GetData" UserSourceName="Fill">
                <SelectCommand>
                  <DbCommand CommandType="Text" ModifiedByUser="true">
                    <CommandText>SELECT        c.nombre_cliente + ' ' + c.apellido_cliente AS Cliente, COUNT(*) AS Cantidad, MONTH(e.fecha_entrada) AS Mes, YEAR(e.fecha_entrada) AS Año
FROM            ENTRADAS AS e INNER JOIN
                         CLIENTES AS c ON e.id_cliente = c.id_cliente
WHERE        (c.id_cliente IN
                             (SELECT        c.id_cliente
                               FROM            COMPROBANTES AS comp INNER JOIN
                                                         DETALLES_COMPROBANTES AS dc ON comp.id_detalle_comprobante = dc.id_detalle_comprobante INNER JOIN
                                                         FORMAS_PAGOS AS fc ON fc.id_forma_pago = dc.id_forma_pago INNER JOIN
                                                         ENTRADAS AS e ON e.id_entrada = comp.id_entrada
                               WHERE        (c.id_cliente = e.id_cliente) AND (fc.forma_pago = 'Efectivo'))) AND (DATEDIFF(MONTH, e.fecha_entrada, GETDATE()) = 1)
GROUP BY c.nombre_cliente + ' ' + c.apellido_cliente, MONTH(e.fecha_entrada), YEAR(e.fecha_entrada)
HAVING        (COUNT(*) &gt; 10)</CommandText>
                    <Parameters />
                  </DbCommand>
                </SelectCommand>
              </DbSource>
