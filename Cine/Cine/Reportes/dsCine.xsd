              <DbSource ConnectionRef="CINE_CORDOBAConnectionString (Settings)" DbObjectName="CINE_CORDOBA.dbo.CLASIFICACIONES" DbObjectType="Table" FillMethodModifier="Public" FillMethodName="Fill" GenerateMethods="Both" GenerateShortCommands="false" GeneratorGetMethodName="GetData" GeneratorSourceName="Fill" GetMethodModifier="Public" GetMethodName="GetData" QueryType="Rowset" ScalarCallRetval="System.Object, mscorlib, Version=4.0.0.0, Culture=neutral, PublicKeyToken=b77a5c561934e089" UseOptimisticConcurrency="true" UserGetMethodName="GetData" UserSourceName="Fill">
                <SelectCommand>
                  <DbCommand CommandType="Text" ModifiedByUser="true">
                    <CommandText>SELECT        c.clasificacion AS Clasificación, c.descripcion AS 'Descripción', COUNT(*) AS Concurrencia, 'Hace 3 meses ' AS Meses
FROM            ENTRADAS AS e INNER JOIN
                         FUNCIONES AS f ON e.id_funcion = f.id_funcion INNER JOIN
                         PELICULAS AS p ON p.id_pelicula = f.id_pelicula INNER JOIN
                         CLASIFICACIONES AS c ON c.id_clasificacion = p.id_clasificacion
WHERE        (DATEDIFF(month, e.fecha_entrada, GETDATE()) = 3)
GROUP BY c.clasificacion, c.descripcion
HAVING        (COUNT(*) &gt; 50)
UNION
SELECT        c.clasificacion, c.descripcion, COUNT(*) AS Expr1, 'Hace 2 meses ' AS Expr2
FROM            ENTRADAS AS e INNER JOIN
                         FUNCIONES AS f ON e.id_funcion = f.id_funcion INNER JOIN
                         PELICULAS AS p ON p.id_pelicula = f.id_pelicula INNER JOIN
                         CLASIFICACIONES AS c ON c.id_clasificacion = p.id_clasificacion
WHERE        (DATEDIFF(month, e.fecha_entrada, GETDATE()) = 2)
GROUP BY c.clasificacion, c.descripcion
HAVING        (COUNT(*) &gt; 50)
ORDER BY 3, 2</CommandText>
                    <Parameters />
                  </DbCommand>
                </SelectCommand>
              </DbSource>
